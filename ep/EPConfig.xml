<?xml version="1.0" encoding="UTF-8"?>

<!-- Basic (minimal) Event Packager configuration.  Doesn't do a whole
     lot, admittedly.  See the examples for more comprehensive setups. -->

<EventPackagerConfiguration>
  <Inputters>
    <Inputter Name="ConsoleInput1" Type="psl.xues.ep.input.ConsoleInput" />
  </Inputters>
  <Transforms>
  </Transforms>
  <Outputters>
    <Outputter Name="ConsoleOutput1" Type="psl.xues.ep.output.ConsoleOutput" />
  </Outputters>
  <Stores>
  </Stores>
  <Rules>
    <Rule Name="ConsoleRule">
      <Inputs>
	<Input Name="ConsoleInput1" />
      </Inputs>
      <Outputs>
	<Output Name="ConsoleOutput1" />
      </Outputs>
    </Rule>
  </Rules>
</EventPackagerConfiguration>
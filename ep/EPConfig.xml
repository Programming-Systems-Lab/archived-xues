<EventPackagerConfiguration>
    <EventFormats>
        <EventFormat Name="HierarchicalEvent"></EventFormat>
        <EventFormat Name="SienaEvent"></EventFormat>
        <EventFormat Name="FlatEvent"></EventFormat>
    </EventFormats>
    <Inputters>
        <Inputter Name="SienaInput1" Type="SienaInput" SienaHost="senp://localhost:7890">
            <SienaFilter Name="TestFilter1">
                <SienaConstraint AttributeName="TestAttribute" Op="EQ" ValueType="String" Value="TestValue"></SienaConstraint>
            </SienaFilter>
            <SienaFilter Name="TestFilter2">
                <SienaConstraint AttributeName="TestAttribute" Op="EQ" ValueType="String" Value="TestValue"></SienaConstraint>
            </SienaFilter>
        </Inputter>
    </Inputters>
<Outputters>
        <Outputter Name="SienaOutput1" Type="SienaOutput" SienaHost="senp://localhost:7891"></Outputter>
    </Outputters>
<Transforms></Transforms>
    <Rules>
        <Rule><!--Sources specify a disjoint set-->
            <Inputs>
                <Input Name="SienaInput1"></Input>
            </Inputs>
            
            <!--Transforms are applied in order-->
            <Transforms></Transforms>
            <Outputs>
                <Output Name="SienaOutput1"></Output>
            </Outputs>
        </Rule>
    </Rules>
    </EventPackagerConfiguration>

